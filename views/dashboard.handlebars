<div class="dashboard-flex">
    <h1 class="tech-blog-logo">Hello, {{user.name}}!</h1>
    <h2 class="your-dashboard">Your Dashboard</h2>
    <div class="dash-container">
        <ul>
            {{#each posts as |post|}}
            <li class="dash-fill"><a class="dashboard-link" href="/updatePost/{{post.id}}">{{post.title}}</a></li>
            {{/each}}
        </ul>
    </div>
   
    <a href="/createPost">Create Post</a>
</div>
